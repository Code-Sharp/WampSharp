<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Lykke WampSharp Microsoft.AspNetCore.WebSockets.Server support</Description>
    <AssemblyTitle>Lykke WampSharp AspNetCore WebSockets Server support</AssemblyTitle>
    <Authors>Lykke</Authors>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>WampSharp.AspNetCore.WebSockets.Server</AssemblyName>
    <PackageId>Lykke.WampSharp.AspNetCore.WebSockets.Server</PackageId>
    <PackageTags>websockets;wampws;rpc;pubsub;wampv1;wampv2;aspnetcore</PackageTags>
    <PackageProjectUrl>https://github.com/LykkeCity/WampSharp</PackageProjectUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore" Version="2.0.0" />
  </ItemGroup>

  
  <ItemGroup>
    <ProjectReference Include="..\WampSharp.WebSockets\WampSharp.WebSockets.csproj" />
  </ItemGroup>

  

</Project>
