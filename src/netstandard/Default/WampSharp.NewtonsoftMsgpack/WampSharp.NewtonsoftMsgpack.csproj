<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <title>WampSharp Newtonsoft.Msgpack binding</title>
    <description>WampSharp Newtonsoft.Msgpack support</description>
    <PackageTags>$(PackageTags) newtonsoft msgpack</PackageTags>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Msgpack" Version="0.1.11" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\WampSharp.NewtonsoftJson\WampSharp.NewtonsoftJson.csproj" />
  </ItemGroup>
</Project>