<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <title>WampSharp NewtonsoftMessagePack binding</title>
    <description>WampSharp NewtonsoftMessagePack support</description>
    <PackageTags>$(PackageTags) newtonsoft messagepack</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NewtonsoftMessagePack" Version="0.1.11" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\WampSharp.NewtonsoftJson\WampSharp.NewtonsoftJson.csproj" />
  </ItemGroup>

</Project>
