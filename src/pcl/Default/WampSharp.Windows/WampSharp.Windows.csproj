<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net46</TargetFramework>
    <RootNamespace>WampSharp</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Debug\</OutputPath>
    <DefineConstants>TRACE;DEBUG;NET45;PCL</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <DocumentationFile>bin\Debug\WampSharp.Windows.XML</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>pdbonly</DebugType>
    <Optimize>true</Optimize>
    <OutputPath>bin\Release\</OutputPath>
    <DefineConstants>TRACE;NET45;PCL</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <DocumentationFile>bin\Release\WampSharp.Windows.XML</DocumentationFile>
  </PropertyGroup>
  <Import Project="$(MSBuildExtensionsPath32)\Microsoft\Portable\$(TargetFrameworkVersion)\Microsoft.Portable.CSharp.targets" />
  <ItemGroup>
    <ProjectReference Include="..\..\WampSharp\WampSharp.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="..\..\..\net45\Default\WampSharp.Windows\Properties\AssemblyInfo.cs">
      <Link>Properties\AssemblyInfo.cs</Link>
    </Compile>
    <Compile Update="..\..\..\net45\Default\WampSharp.Windows\WAMP2\V2\Fluent\MessageWebSocketActivator.cs">
      <Link>WAMP2\V2\Fluent\MessageWebSocketActivator.cs</Link>
    </Compile>
    <Compile Update="..\..\..\net45\Default\WampSharp.Windows\WAMP2\V2\Fluent\MessageWebSocketChannelFactoryExtensions.cs">
      <Link>WAMP2\V2\Fluent\MessageWebSocketChannelFactoryExtensions.cs</Link>
    </Compile>
    <Compile Update="..\..\..\net45\Default\WampSharp.Windows\Windows\MessageWebSocketConnection.cs">
      <Link>Windows\MessageWebSocketConnection.cs</Link>
    </Compile>
    <Compile Update="..\..\..\net45\Default\WampSharp.Windows\Windows\MessageWebSocketTextConnection.cs">
      <Link>Windows\MessageWebSocketTextConnection.cs</Link>
    </Compile>
    <Compile Update="..\..\..\net45\Default\WampSharp.Windows\Windows\MessageWebSocketBinaryConnection.cs">
      <Link>Windows\MessageWebSocketBinaryConnection.cs</Link>
    </Compile>
  </ItemGroup>
</Project>